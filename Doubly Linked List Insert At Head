#include <iostream>
using namespace std;

struct node {
	int x;
	node* next;
	node* previous;
};

node* head = NULL;

void storeNodes () {
	head = new node;
	head->x = 10;
	head->previous = NULL;
	node* n1 = new node;
	n1->x = 20;
	n1->previous = head;
	head->next = n1;
	node* n2 = new node;
	n2->x = 30;
	n2->previous = n1;
	n1->next = n2;
	node* tail = new node;
	tail->x = 40;
	tail->previous = n2;
	n2->next = tail;
	tail->next = NULL;
}

void insAtHead () {
	node* temp = new node;
	cout << "Enter New Head:" << endl;
	cin >> temp->x;
	temp->next = head;
	head = temp;
	temp->previous = NULL;
}

void display () {
	node* dis = head;
	while (dis != NULL) {
		cout << dis->x << "  ";
		dis = dis->next;
	}
	cout << endl;
}

int main () {
	storeNodes();
	insAtHead();
	display();
}
