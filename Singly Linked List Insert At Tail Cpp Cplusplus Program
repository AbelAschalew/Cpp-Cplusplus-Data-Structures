#include <iostream>
using namespace std;

struct node {
	int x;
	node* next;
};

node* head = NULL;

void insAtTail () {
	head = new node;
	head->x = 10;
	head->next = NULL;
	node* temp = new node;
	cout << "Enter Item:" << endl;
	cin >> temp->x;
	node* srch = head;
	while (srch->next != NULL) {
		srch = srch->next;
	}
	srch->next = temp;
	temp->next = NULL;
}

void display () {
	node* dis = head;
	while (dis != NULL) {
		cout << dis->x << "  ";
		dis = dis->next;
	}
	cout << endl;
}

int main () {
	insAtTail();
	display();
}
